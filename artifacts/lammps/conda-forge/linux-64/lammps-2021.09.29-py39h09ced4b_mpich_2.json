 "raw_recipe": "{% set build = 2 %}\n{% set name = \"lammps\" %}\n{% set version = \"stable_29Sep2021\" %}\n{% set sha256 = \"2dff656cb21fd9a6d46c818741c99d400cfb1b12102604844663b655fb2f893d\" %}\n{% set date = datetime.datetime.strptime(version.split('_')[1], \"%d%b%Y\") %}\n{% set conda_version = \"{:%Y.%m.%d}\".format(date) %}\n\npackage:\n  name: {{ name|lower }}\n  version: {{ conda_version }}\n\nsource:\n  - fn: {{ name }}-{{ conda_version }}.tar.gz\n    url: https://github.com/lammps/lammps/archive/{{ version }}.tar.gz\n    sha256: {{ sha256 }}\n    patches: \n      - macos_install.patch\n      - vcsgc_mtp_n2p2.patch\n      - mliappy.patch\n  - fn: vcsgc-lammps.tar.gz\n    url: https://vcsgc-lammps.materialsmodeling.org/vcsgc-lammps.tgz\n    sha256: 9227413d3f1ef4205bd06b48f91df4b461dccf88540c756e43ab848df908a6ba\n    folder: vcsgc-lammps\n  - fn: mlip.tar.gz\n    url: https://gitlab.com/ashapeev/mlip-1-legacy/-/archive/master/mlip-1-legacy-master.tar.gz\n    sha256: b7b8624ea9e1ee57bdc083e214c607c0de8a87643dcdc8d56c6c7084a4109f68\n    folder: mlip\n    patches:\n      - domain.patch\n    \nbuild:\n  number: {{ build }}\n  string: py{{ CONDA_PY }}h{{ PKG_HASH }}_{{ mpi }}_{{ build }} \n  skip: True  # [win]\n\nrequirements:\n  build:\n    - {{ compiler('c') }}\n    - {{ compiler('cxx') }}\n    - {{ compiler('fortran') }}\n    - cmake\n    - make\n    - pkg-config\n  \n  host:\n    - python\n    - cython\n    - setuptools\n    - {{ mpi }}\n    - zlib\n    - fftw\n    - hdf5\n    - libpng\n    - libcblas  # [linux]\n    - libblas  # [linux]\n    - liblapack  # [linux]\n    - kim-api\n    - libcurl\n    - mpi4py\n    - mlip  # [linux]\n    - n2p2 =2.1.4  # [linux]\n    - quippy =0.0.1=*_4  # [linux]\n\n  run:\n    - python\n    - setuptools\n    - {{ mpi }}\n    - kim-api\n    - mlip  # [linux]\n    - n2p2 =2.1.4  # [linux]\n    - quippy =0.0.1=*_4  # [linux]\n\ntest:\n  imports:\n    - lammps\n  commands: \n    - lmp_serial -help > help_serial.txt\n    - mpiexe