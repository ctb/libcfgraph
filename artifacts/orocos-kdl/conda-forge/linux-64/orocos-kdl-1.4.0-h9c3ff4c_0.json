 "raw_recipe": "{% set name = \"orocos-kdl\" %}\n{% set version = \"1.4.0\" %}\n\npackage:\n  name: {{ name }}\n  version: {{ version }}\n\nsource:\n  url: https://github.com/orocos/orocos_kinematics_dynamics/archive/v{{ version }}.tar.gz\n  sha256: 05b93e759923684dc07433ccae1e476d158d89b3c2be5079c20062406da7b4dd\n  patches:\n    - orocos_kdl.patch\n    - python_orocos_kdl.patch\n    - python_osx.patch                    # [osx]\n    - assignment_operator.patch           # [unix]\n    - crosscompile_sip_include_dir.patch  # [build_platform != target_platform]\n\nbuild:\n  number: 0\n  skip: true  # [win and vc<14]\n  script: \"echo 'do nothing'\"  # [unix]\n  run_exports:\n    - {{ pin_subpackage('orocos-kdl', max_pin='x.x') }}\n    - {{ pin_subpackage('python-orocos-kdl', max_pin='x.x') }}\n\noutputs:\n  - name: orocos-kdl\n    version: {{ version }}\n    script: buildpkg.sh  # [unix]\n    script: bldpkg.bat   # [win]\n    requirements:\n      build:\n        - {{ compiler('cxx') }}\n        - {{ compiler('c') }}\n        - cmake\n        - ninja       # [win]\n        - make        # [not win]\n        - pkg-config\n      host:\n        - eigen\n      run:\n        - eigen\n    test:\n      commands:\n        - if not exist %LIBRARY_PREFIX%\\\\lib\\\\orocos-kdl.lib exit 1  # [win]\n        - if not exist %LIBRARY_PREFIX%\\\\lib\\\\orocos-kdl.dll exit 1  # [win]\n        - test -f $PREFIX/lib/liborocos-kdl${SHLIB_EXT}              # [unix]\n  - name: python-orocos-kdl\n    version: {{ version }}\n    script: buildpkg.sh  # [unix]\n    script: bldpkg.bat   # [win]\n    requirements:\n      build:\n        - python                                 # [build_platform != target_platform]\n        - cross-python_{{ target_platform }}     # [build_platform != target_platform]\n        - {{ compiler('cxx') }}\n        - {{ compiler('c') }}\n        - cmake\n        - ninja  # [win]\n        - make  # [not win]\n        - pkg-config\n        - sip                            