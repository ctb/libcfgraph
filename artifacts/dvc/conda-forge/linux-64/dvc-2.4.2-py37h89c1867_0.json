 "raw_recipe": "{% set name = \"dvc\" %}\n{% set version = \"2.4.2\" %}\n\npackage:\n  name: {{ name|lower }}\n  version: {{ version }}\n\nsource:\n  url: https://pypi.io/packages/source/{{ name[0] }}/{{ name }}/{{ name }}-{{ version }}.tar.gz\n  sha256: 253f46713381318e9ec512795bf15bee8eae5a54b0009784ee424c4a4c5d850d\n  patches:\n    - build.patch\n\n# The \"true\" build is specified in the dvc output below.\n# Of the build properties here and below:\n# number is propagated into the outputs below\nbuild:\n  number: 0\n  skip: true  # [py<=36 and win]\n\noutputs:\n  - name: dvc\n    build:\n      entry_points:\n        - dvc = dvc.main:main\n      script:\n        - python -m pip install . --no-deps -vv\n\n    # Base package supports dvc, but only supports built-in remotes\n    test:\n      requires:\n        - git\n        - pip\n      imports:\n        - dvc\n      commands:\n        - pip check\n        - dvc version\n        - 'dvc version | grep \"Supports: http, https\"'\n\n    requirements:\n      host:\n        - python\n        - pip\n      run:\n        - python\n        - ply >=3.9\n        - colorama >=0.3.9\n        - configobj >=5.0.6\n        - gitpython >3\n        - dulwich >=0.20.23\n        - pygit2 >=1.5.0\n        - setuptools >=34.0.0\n        - nanotime >=0.5.2\n        - pyasn1 >=0.4.1\n        - voluptuous >=0.11.7\n        - jsonpath-ng >=1.5.1\n        - requests >=2.22.0\n        - grandalf ==0.6\n        - distro >=1.3.0\n        - appdirs >=1.4.3\n        - ruamel.yaml >=0.16.1\n        - toml >=0.10.1\n        - funcy >=1.14\n        - pathspec >=0.6.0\n        - shortuuid >=0.5.0\n        - tqdm >=4.45.0,<5\n        - packaging >=19.0\n        - zc.lockfile >=1.2.1\n        - flufl.lock >=3.2,<4\n        - win_unicode_console >=0.5  # [win]\n        - pywin32 >=225  # [win]\n        - networkx ~=2.5\n        - psutil >=5.8.0\n        - pydot >=1.2.4\n        - speedcopy >=2.0.1  # [py<38 and win]\n        - dataclasses ==0.7  # [py<37